{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Couchbase",
  "configuration-groups": [
    {
      "label": "General",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "csv",
          "label": "Nodes",
          "name": "nodes"
        },
        {
          "widget-type": "textbox",
          "label": "Bucket",
          "name": "bucket"
        },
        {
          "widget-type": "input-field-selector",
          "label": "Key Field",
          "name": "keyField"
        },
        {
          "widget-type": "radio-group",
          "name" : "operation",
          "label" : "Operation",
          "widget-attributes": {
            "layout": "inline",
            "default": "INSERT",
            "options": [
              {
                "id": "INSERT",
                "label": "Insert"
              },
              {
                "id": "REPLACE",
                "label": "Replace"
              },
              {
                "id": "UPSERT",
                "label": "Upsert"
              }
            ]
          }
        }
      ]
    },
    {
      "label": "Credentials",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "username"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    },
    {
      "label": "Advanced",
      "properties": [
        {
          "widget-type": "number",
          "label": "Batch Size",
          "name": "batchSize",
          "widget-attributes": {
            "min": "0",
            "default": "100"
          }
        }
      ]
    }
  ],
  "outputs": [],
  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }
}
